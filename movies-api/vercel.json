{
  "name": "movies-api",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@movies-api-db-user",
    "DB_PASSWORD": "@movies-api-db-password",
    "DB_HOST": "@movies-api-db-host",
    "DB_NAME": "@movies-api-db-name"
  }
}
